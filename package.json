    "url": "https://github.com/highstone-team/hyrai-icons/issues"
  },
  "homepage": "https://github.com/highstone-team/hyrai-icons#readme",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "3.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@svgr/cli": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
