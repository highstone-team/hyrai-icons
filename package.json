{
  "name": "@highstone/hyrai-icons",
  "version": "1.0.1",
  "description": "Icons used in hyrai project",
  "main": "dist/icons/",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
